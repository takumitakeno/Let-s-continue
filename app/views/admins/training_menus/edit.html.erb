<div class="container">
  <div class="row">
    <div class="col-xs-2">
      <p>ここは管理人専用です</p>
    </div>
  </div>
  <div class="row">
    <%= render 'admins/homes/admin_menu' %>
    <div class="col-xs-9">
      <div class="panel panel-warning">
        <div class="panel-heading">
          <h4>トレーニングメニュー登録</h4>
        </div>
        <div class="panel-body">
          <%= form_for(@training_menu, url: admins_training_menu_path) do |f| %>
            <%= render 'layouts/error_messages', model: f.object %>
            <%= f.text_field :menu, class: "form-control" %><br>
            <%= f.submit "変更する", class: "btn btn-primary btn-block" %>
          <% end %>
        </div>
      </div>
    </div>
  </div>
</div>